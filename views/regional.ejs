<%- include('header'); -%>
<div class="col-12">
    <div class="row">  
      <label for="cAr">Country & Region</label>
      <select name="cAr" id="cAr" class="selectpicker form-control"
      data-style="btn-success" multiple data-live-search="true" >
      <% latest.forEach((e)=>{  %>
      <option class="text-black" value="<%= e.countryregion %>"><%= e.countryregion %><% if(e.provincestate != ""){%> (<%= e.provincestate %>)<% } %></option>
      <% }); %>
     </select>
  </div>
</div>

<div class="col-12">
    <div class="card">
      <div class="card-header">

        <div class="row"><br><br>
            <div class="col-sm-6 ">
            </div>
            <div class="col-sm-6 ">
              <div class="card-title float-right text-lg text-white">
                <h3>Confirmed Cases</h3>
              </div>
            </div>
          </div>  
    </div>
      <div class="card-body">
        <div class="chart-area1"><div class="chartjs-size-monitor" style="position: absolute; left: 0px; top: 0px; right: 0px; bottom: 0px; overflow: hidden; pointer-events: none; visibility: hidden; z-index: -1;">
            <div class="chartjs-size-monitor-expand" style="position:absolute;left:0;top:0;right:0;bottom:0;overflow:hidden;pointer-events:none;visibility:hidden;z-index:-1;">
                <div style="position:absolute;width:1000000px;height:1000000px;left:0;top:0">
                </div>
            </div>
            <div class="chartjs-size-monitor-shrink" style="position:absolute;left:0;top:0;right:0;bottom:0;overflow:hidden;pointer-events:none;visibility:hidden;z-index:-1;">
                <div style="position:absolute;width:200%;height:200%;left:0; top:0"></div>
            </div>
        </div>
          <canvas id="chartBig1" width="854" height="300" class="chartjs-render-monitor" style="display: block; width: 854px; height: 300;"></canvas>
        </div>
      </div>
    </div>
